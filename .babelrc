{
  "presets": [
    [ "@babel/preset-env", {
      "targets": {
        "browsers": [
          "last 2 versions"
        ]
      },
      "useBuiltIns": "usage"
    }],
    ["@babel/stage-0", {
      "decoratorsLegacy": true
    }],
    "@babel/react"
  ],
  "plugins": [
    "@babel/transform-runtime"
  ],
  "sourceMaps": true
}