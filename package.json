{
  "name": "portfolio",
  "version": "1.3.8",
  "description": "www.spencerkline.com",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development --devtool inline-module-source-map --watch",
    "start": "superstatic -p 5000 --host 127.0.0.1 --no-cache --compression build",
    "build": "cross-env NODE_ENV=production webpack -p --progress",
    "prebuild": "rm -rf build mkdirp build",
    "lighthouse": "lighthouse --output-path=analytics/lighthouse.html",
    "test": "jest --coverage --no-cache",
    "test:watch": "npm run -s test -- --watch"
  },
  "keywords": [
    "react",
    "sass",
    "tachyons",
    "babel",
    "apollo",
    "graphql",
    "jest",
    "lighthouse"
  ],
  "license": "MIT",
  "author": "Spencer Kline <dskline1@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/dskline/portfolio.git"
  },
